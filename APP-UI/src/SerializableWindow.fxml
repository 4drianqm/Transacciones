<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="Serializable_window" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SerializableWindowController">
   <children>
      <TableView fx:id="tbl_Serializable" layoutX="8.0" layoutY="93.0" prefHeight="243.0" prefWidth="577.0">
        <columns>
          <TableColumn fx:id="ID" prefWidth="51.0" text="ID" />
          <TableColumn fx:id="Nombre" prefWidth="157.0" text="Nombre" />
            <TableColumn fx:id="Curso" prefWidth="163.0" text="Curso" />
            <TableColumn fx:id="Creditos" prefWidth="205.0" text="Creditos" />
        </columns>
      </TableView>
      <Button fx:id="btn_agregar" layoutX="169.0" layoutY="342.0" mnemonicParsing="false" onAction="#executeQSL" prefHeight="36.0" prefWidth="76.0" text="Agregar" />
      <TextField fx:id="txt_nombre" layoutX="31.0" layoutY="39.0" promptText="nombre" />
      <TextField fx:id="txt_curso" layoutX="198.0" layoutY="40.0" promptText="Curso" />
      <TextField fx:id="txt_creditos" layoutX="374.0" layoutY="41.0" promptText="Creditos" />
      <Button fx:id="btnSerializable_commit" layoutX="262.0" layoutY="342.0" mnemonicParsing="false" onAction="#TransactionCommit" prefHeight="36.0" prefWidth="76.0" text="Commit" />
      <Button fx:id="btnSerializable_Rollback" layoutX="350.0" layoutY="342.0" mnemonicParsing="false" onAction="#Transaction_Rollback" prefHeight="36.0" prefWidth="76.0" text="Rollback" />
      <Button fx:id="btn_inicarTrasaccion" layoutX="31.0" layoutY="342.0" mnemonicParsing="false" onAction="#Init_Transaction" prefHeight="36.0" prefWidth="126.0" text="Inicar Transaccion" />
      <Button fx:id="btn_actualizar" layoutX="442.0" layoutY="342.0" mnemonicParsing="false" onAction="#Actualizar" prefHeight="36.0" prefWidth="76.0" text="Actualizar" />
   </children>
</AnchorPane>
